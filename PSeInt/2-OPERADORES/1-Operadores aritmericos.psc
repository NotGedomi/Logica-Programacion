Algoritmo OperadoresAritmericos
	// Operadores Aritmeticos
	// Suma +
	a = 3 + 4
	Imprimir 'Suma: ', a
	
	// Resta-
	b = 4 - 3
	Imprimir 'Resta: ', b
	
	// Multiplicacion
	c = a * 2
	Imprimir 'Multiplicacion: ', c
	
	// Division
	d = a / b
	Imprimir  'Division: ', d

	// Potencia
	e = a ^ 2
	Imprimir 'Potencia: ', e
	
	// Modulo (residuo division)
	f = 6 % 2
	Imprimir 'Modulo: ', f
	
FinAlgoritmo
